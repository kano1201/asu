<h1>投稿一覧</h1>
<%= @photos.each do |photo| %>
  <div>
    <%= link_to photo_path(photo.id) do %>
      <%= attachment_image_tag photo, :image %>
    <% end %>
    <%= link_to user_path(photo.user) do %>
      <p><%= photo.user.couple_name %></p>
    <% end %>
    <p><%= photo.title %></p>
    <p><%= link_to "#{photo.comments.count}Comments", photo_path(photo.id) %></p>
    <p><%= photo.likes.count %>Likes</p>
  </div>
<% end %>