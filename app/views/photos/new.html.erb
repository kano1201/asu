<h1>新規投稿</h1>
<% if @photo.errors.any? %>
 <%= @photo.errors.count %>errors
 <% @photo.errors.full_messages.each do |message| %>
  <%= message %>
 <% end %>
<% end %>

<%= form_with model: @photo, local:true do |f| %>
 <h4>画像</h4>
 <%= f.attachment_field :image %>
 <h4>タイトル</h4>
 <%= f.text_field :title %>
 <h4>内容</h4>
 <%= f.text_field :body %>
 <h4>場所</h4>
 <%= f.text_field :place %>
 <%= f.submit '投稿' %>
<% end %>