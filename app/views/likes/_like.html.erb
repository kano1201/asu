<% if photo.liked_by?(current_user) %>
  <%= link_to photo.likes.count, photo_likes_path(photo), method: :delete,remote: true, class: "fas fa-heart heart_red" %>
<% else %>
  <%= link_to photo.likes.count, photo_likes_path(photo), method: :post,remote: true, class: "fas fa-heart heart_blue" %>
<% end %>