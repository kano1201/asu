<% users.each do |user| %>
  <% unless user.is_deleted == true %>
    <div class="col-md-2.5 ml-2 mr-2">
      <div class="row mt-3">
        <div class="col-md-12 text-left">
          <%= attachment_image_tag(user, :profile_image,:fill, 150, 150, fallback: "no_image.jpg", size: "150x150")%>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12 text-left" style="margin-top: 20px;">
          <%= link_to user.couple_name,user_path(user.id), 'data-turbolinks': false %>
        </div>
      </div>
    </div>
  <% end %>
<% end %>