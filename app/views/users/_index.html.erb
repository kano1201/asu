<div class="row mt-3">
  <% users.each do |user| %>
    <div class="col-md-2.5" style="margin-left: 30px;">
      <div class="row mt-3">
        <div class="col-md-12 text-left">
          <%= attachment_image_tag(user, :profile_image,:fill, 150, 150, fallback: "no_image.jpg", size: "150x150")%>
        </div>
      </div>
      <div class="row">
        <div class="col-md-12 text-left" style="margin-top: 20px;">
          <%= link_to user.couple_name,user_path(user.id) %>
        </div>
      </div>
    </div>
  <% end %>
</div>